	IF THEN ELSE(climate scenario chooser=1, ref temperature[location]+projected temperature change\
		[moderate],
	
	IF THEN ELSE(climate scenario chooser=2, ref temperature[location]+projected temperature change\
		[high],0
	
	)))
	~	C
	~		|

precipitation[location]=
	IF THEN ELSE(climate scenario chooser=0, ref precipitaion[location],
	
	IF THEN ELSE(climate scenario chooser=1, ref precipitaion[location]*(1+projected precipitation change\
		[moderate]/100),
	
	IF THEN ELSE(climate scenario chooser=2, ref precipitaion[location]*(1+projected precipitation change\
		[high]/100),0
	
	)))
	~	mm
	~		|

