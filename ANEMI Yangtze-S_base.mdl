desired renewable energy share[location]=
	(desired energy share[location,hydro]+desired energy share[location,nuclear]+desired energy share\
		[location,new])/SUM(desired energy share
	
	[location,energy source!])
	~	dmnl
	~		|

per hectare consumption[location]=
	IF THEN ELSE(turn on water technological change=0, (1-base returnable water/100) * base specific water intake\
		[location]
	
	 * "2015 tec change for consumption in agr" * temperature feedback[location], (1-base returnable water\
		/100) * base specific water intake
	
	[location] * tec change for consumption in agr * temperature feedback[location])
	~	m*m*m/ha/Year
	~		|

coal requirement[location]=
