	hist low temp
	~	C
	~		|

precipitation multiplier[location]=
	base precipitation multiplier * (temperature[location]-ref temperature[location])
	~	percent
	~		|

ref precipitaion[location]=
	1037, 1421, 1203
	~	mm
	~	average precipitaion from 1960 to 2006
	|

climate scenario chooser=
	0
	~	
	~	climate scenario chooser=0, no climate change considered \
		(precipitation/temperature use historical average value);   climate \
		scenario chooser=1, climate change (RCP4.5) shall be used;   climate \
		scenario chooser=2, climate change (RCP8.5) shall be used.
	|

adaptation rate[location,damage]=
	(temperature[location] - Adapted Temperature[location,damage]) * "fractional adaptation rate=0"\
		[damage]
	~	degC/yr
	~	Rate of adaptation to altered climatic conditions.
	|

climate damage effect[location,damage]=
	1/(1+climate damage scale[damage]*((temperature[location]-Adapted Temperature[location\
		,damage])/ref temperature[location])^climate damage nonlinearity[damage])
	~	dmnl
	~	Multiplier for climate damage effect on output (tangible) or environmental \
		services (intangible).
	|

new electricity requirement[location]=
	total aggregate energy requirement[location]*desired energy share[location,new]
	~	tce/yr
	~		|

gas requirement[location]=
	total aggregate energy requirement[location]*desired energy share[location,gas]
	~	tce/yr
	~		|

hydro electricity requirement[location]=
	total aggregate energy requirement[location]*desired energy share[location,hydro]
	~	tce/yr
	~		|

per hectare withdrawals[location]=
	IF THEN ELSE(turn on water technological change=0, base specific water intake[location\
		]*"2015 tec change for withdrawal in agr"
	
	*temperature feedback[location], base specific water intake[location] * tec change for withdrawal in agr\
		 * temperature feedback
	
	[location])
	~	m3/Hectare/yr
	~		|

